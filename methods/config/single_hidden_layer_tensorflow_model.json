{   
    "name": "one_layer_tf",
    "class": "methods.SingleHiddenLayerTensorflowModel",
    "hyperparameters": {
        "hidden_layer_units": {
            "type": "int",
            "range": [2, 128]
        },
        "epochs": {
            "type": "int",
            "range": [10, 1000]
        },
        "learning_rate": {
            "type": "float_exp",
            "range": [1e-5, 1e-1]
        },
        "batch_size": {
            "type": "int_cat",
            "values": [1, 2, 4, 8, 16, 32, 64, 128]
        }
    },
    "root_hyperparameters": ["hidden_layer_units", "epochs", "learning_rate", "batch_size"],
    "conditional_hyperparameters": {}
}
